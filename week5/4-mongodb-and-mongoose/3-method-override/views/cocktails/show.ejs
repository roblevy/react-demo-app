<main>
  <h1><%= name %></h1>
  <p>Ingredients:
    <% ingredients.forEach(ingredient => { %>
      <span class="tag"><%= ingredient %></span>
      <% }) %>
  </p>
  <img src="<%= image %>"/>
  <form action="/cocktails/<%= _id %>?_method=DELETE" method="post">
    <button>Delete</button>
  </form>
</main>
